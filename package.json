{
  "name": "bundesliga-manager",
  "version": "1.6.2",
  "private": true,
  "scripts": {
    "dev": "export NODE_ENV=development APP_ENV=development; npm install; node scripts/entrypoint.mjs & next dev",
    "build": "npm ci; next build",
    "start": "export NODE_ENV=production APP_ENV=production; npm run start2",
    "start:test": "export NODE_ENV=test APP_ENV=test; npm run start2",
    "start2": "node scripts/entrypoint.mjs & next build; next start",
    "lint": "next lint --max-warnings 0",
    "pretty": "prettier --check .",
    "format": "prettier --write .",
    "cypress:open": "cypress open",
    "cypress": "wait-on http://127.0.0.1:3000; cypress run",
    "start:no-build": "node scripts/entrypoint.mjs & next start"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.8",
    "@socialgouv/matomo-next": "1.4.0",
    "@types/node": "18.7.18",
    "@types/react": "18.0.21",
    "bcryptjs": "2.4.3",
    "csv-stringify": "6.2.0",
    "dotenv": "16.0.3",
    "mysql": "2.18.1",
    "next": "12.3.1",
    "next-auth": "4.12.3",
    "next-pwa": "5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.31.1",
    "sqlite": "4.1.2",
    "sqlite3": "5.1.2",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "cypress": "10.9.0",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-cypress": "2.12.1",
    "prettier": "2.7.1",
    "wait-on": "6.0.1"
  }
}
